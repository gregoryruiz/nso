{
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": "src",
    "module": "es2015",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictNullChecks": false,
    "outDir": "out-tsc",
    "skipLibCheck": true,
    "sourceMap": true,
    "sourceRoot": "",
    "target": "es2015",
    "lib": [
      "dom",
      "es2015",
      "es2016",
      "es2017"
    ],
    "typeRoots": [
      "./node_modules/@types"
    ],
    "types": [
      "angular",
      "mocha",
      "node",
      "d3"
    ]
  },
  "include": [
    "src"
  ]
}
